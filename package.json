{
    "name": "cphfacilitation",
    "description": "Ghost theme made for cphfacilitation by innovationsanstalten",
    "version": "1.0.1",
    "engines": {
        "ghost": ">=4.0.0",
        "ghost-api": "v4"
    },
    "license": "MIT",
    "screenshots": {
        "desktop": "assets/screenshot-desktop.jpg",
        "mobile": "assets/screenshot-mobile.jpg"
    },
    "scripts": {
        "dev": "gulp",
        "build": "gulp build",
        "zip": "gulp zip",
        "test": "gscan ."
    },
    "author": {
        "name": "innovationsanstalten",
        "email": "kljo@aspit.dk"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Magazine"
        ]
    },
    "keywords": [
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/innovationsanstalten/cphfacilitation-ghost-theme.git"
    },
    "bugs": "https://github.com/innovationsanstalten/cphfacilitation-ghost-theme/issues",
    "contributors": "https://github.com/innovationsanstalten/cphfacilitation-ghost-theme/graphs/contributors",
    "devDependencies": {
        "@tryghost/release-utils": "0.6.12",
        "autoprefixer": "10.2.5",
        "beeper": "2.1.0",
        "cssnano": "4.1.10",
        "gscan": "4.0.0",
        "gulp": "4.0.2",
        "gulp-chmod": "^3.0.0",
        "gulp-concat": "2.6.1",
        "gulp-livereload": "4.0.2",
        "gulp-postcss": "9.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-terser": "^2.0.1",
        "gulp-zip": "5.1.0",
        "inquirer": "8.0.0",
        "postcss": "8.2.8",
        "postcss-color-function": "4.1.0",
        "postcss-custom-properties": "9.0.2",
        "postcss-easy-import": "3.0.0",
        "pump": "3.0.0"
    },
    "browserslist": [
        "defaults"
    ],
    "config": {
        "posts_per_page": 25,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    },
    "dependencies": {}
}
